# Bluetooth
/system/bin/hwaddrs                                  u:object_r:hwaddrs_exec:s0

# Camera
/dev/gemini0                                         u:object_r:camera_device:s0
/data/misc/camera/cam_socket.*                       u:object_r:camera_socket:s0

# Dev block nodes
/dev/block/platform/msm_sdcc\.1/by-name/encrypt      u:object_r:encrypt_block_device:s0
/dev/block/platform/msm_sdcc\.1/by-name/persist      u:object_r:persist_block_device:s0

# GPS
/dev/gss                                             u:object_r:sensors_device:s0

# Sensors
