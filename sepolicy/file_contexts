/dev/socket/mpctl                  u:object_r:mpctl_socket:s0
/dev/gss                           u:object_r:sensors_device:s0

/data/cam_socket.*                 u:object_r:camera_socket:s0

/data/app/sensor_ctl_socket        u:object_r:sensors_socket:s0

/sns(/.*)?                         u:object_r:sensors_persist_file:s0

/sys/devices/virtual/input/lge_touch/knock_on u:object_r:touchpanel_sysfs:s0
/sys/devices/virtual/graphics/fb0/rgb      u:object_r:kcal_sysfs:s0

